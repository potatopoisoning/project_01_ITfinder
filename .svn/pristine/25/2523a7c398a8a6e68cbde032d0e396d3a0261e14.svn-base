@charset "UTF-8";

p,
.tbl,
.do
{
	width: 100%;
	min-width: 1000px;
	max-width: 1200px;
	margin: 0 10% 0 7%;
	border-collapse: collapse;
}
.stbl
{
	width: 100%;
	min-width: 1000px;
	max-width: 1200px;
	margin: 0 10% 0 7%;
	border-collapse: collapse;
	text-align: center;
}

.htbl
{
	width: 100%;
	min-width: 1000px;
	max-width: 1200px;
	margin: 0 10% 0 7%;
	border-collapse: collapse;
}

.tbl th,
.tbl td
{
	padding: 10px 0;
	border: 1px solid #999;
}

.tbl td
{
	text-align: center;
}
.tbl th
{
	background-color: lightgray;
}